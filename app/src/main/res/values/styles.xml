<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="darkroundedet">
        <item name="android:radius">150dp</item>
        <item name="android:background">#eeeeee</item>
        <item name="android:textColor">@color/OurDark</item>
        <item name="android:textColorHint">#444444</item>
        <item name="android:padding">12dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:alpha">0.6</item>
    </style>



    <style name="roundedbutton">
        <item name="android:radius">10dp</item>
        <item name="android:bottomRightRadius">100dp</item>
        <item name="android:topLeftRadius">100dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">#eeeeee</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="cvstyle">
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>

    </style>
</resources>
